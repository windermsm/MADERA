	<queryString>
		<![CDATA[select  a.fecha_ajuste as fecha,
        upper(a.usuario_ajuste) as usuario,
        upper(a.motivo_ajuste) as motivo,
        a.id_producto as id_producto,
        upper(b.desc_producto) as descripcion,
        a.cantidad_ajuste as cantidad,
        a.tipo_ajuste as tipo
from    d_ajuste as a,
        m_producto as b
where   a.id_producto = b.id_producto
    and date_format(a.fecha_ajuste, '%Y-%m-%d') between date_format(str_to_date($P{P_FECHA_INICIAL}, '%Y-%m-%d'), '%Y-%m-%d') and date_format(str_to_date($P{P_FECHA_FINAL}, '%Y-%m-%d'), '%Y-%m-%d')]]>
	</queryString>
